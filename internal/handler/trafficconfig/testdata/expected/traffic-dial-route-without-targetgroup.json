{
  "hosts": [
    "qal-air.httpbin.mesh",
    "qal.httpbin.intuit",
    "qal.httpbin.mesh"
  ],
  "http": [
    {
      "name": "all",
      "match": [
        {
          "headers": {
            "x-intuit-route-name": {
              "exact": "all"
            }
          }
        }
      ],
      "route": [
        {
          "destination": {
            "host": "qal.httpbin.mesh"
          },
          "weight": 10
        },
        {
          "destination": {
            "host": "qal-air.httpbin.mesh"
          },
          "weight": 90
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "testRouteToAccessAirStack",
      "match": [
        {
          "headers": {
            "x-intuit-route-name": {
              "exact": "testRouteToAccessAirStack"
            }
          }
        }
      ],
      "route": [
        {
          "destination": {
            "host": "qal-air.httpbin.mesh"
          },
          "weight": 100
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "Health Check",
      "match": [
        {
          "headers": {
            "x-intuit-route-name": {
              "exact": "Health Check"
            }
          }
        }
      ],
      "route": [
        {
          "destination": {
            "host": "qal.httpbin.mesh"
          },
          "weight": 10
        },
        {
          "destination": {
            "host": "qal-air.httpbin.mesh"
          },
          "weight": 90
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "defaultall-qal-air-mesh",
      "match": [
        {
          "uri": {
            "prefix": "/"
          },
          "authority": {
            "prefix": "qal-air.httpbin.mesh"
          }
        }
      ],
      "route": [
        {
          "destination": {
            "host": "qal-air.httpbin.mesh"
          }
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "defaultall-qal-air",
      "match": [
        {
          "uri": {
            "prefix": "/"
          },
          "authority": {
            "prefix": "qal.httpbin.intuit"
          }
        }
      ],
      "route": [
        {
          "destination": {
            "host": "qal-air.httpbin.mesh"
          }
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "defaultall-qal",
      "match": [
        {
          "uri": {
            "prefix": "/"
          },
          "authority": {
            "prefix": "qal.httpbin.mesh"
          }
        }
      ],
      "route": [
        {
          "destination": {
            "host": "qal.httpbin.mesh"
          }
        }
      ],
      "timeout": "10s"
    }
  ]
}
