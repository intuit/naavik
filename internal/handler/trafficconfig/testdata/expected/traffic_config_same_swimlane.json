{
  "hosts": [
    "e2e.httpbin.mesh",
    "e2e.httpbin.intuit",
    "e2e-air.httpbin.mesh"
  ],
  "http": [
    {
      "name": "v1",
      "match": [{ "headers": { "x-intuit-route-name": { "exact": "v1" } } }],
      "route": [
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 100
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "all",
      "match": [{ "headers": { "x-intuit-route-name": { "exact": "all" } } }],
      "route": [
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 20
        },
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 50
        },
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 30
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "Health Check",
      "match": [
        { "headers": { "x-intuit-route-name": { "exact": "Health Check" } } }
      ],
      "route": [
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 20
        },
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 50
        },
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 30
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "defaultall-e2e",
      "match": [
        {
          "uri": { "prefix": "/" },
          "authority": { "prefix": "e2e.httpbin.mesh" }
        }
      ],
      "route": [
        { "destination": { "host": "e2e.httpbin.mesh" } }
      ],
      "timeout": "10s"
    },
    {
      "name": "defaultall-e2e-air-mesh",
      "match": [
        {
          "uri": { "prefix": "/" },
          "authority": { "prefix": "e2e-air.httpbin.mesh" }
        }
      ],
      "route": [
        { "destination": { "host": "e2e-air.httpbin.mesh" } }
      ],
      "timeout": "10s"
    },
    {
      "name": "defaultall-e2e-air",
      "match": [
        {
          "uri": { "prefix": "/" },
          "authority": { "prefix": "e2e.httpbin.intuit" }
        }
      ],
      "route": [
        { "destination": { "host": "e2e-air.httpbin.mesh" } }
      ],
      "timeout": "10s"
    }
  ]
}
