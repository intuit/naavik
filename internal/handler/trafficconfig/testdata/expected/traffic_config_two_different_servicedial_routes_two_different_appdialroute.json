{
  "hosts": [
    "e2e.httpbin.mesh",
    "e2e-air.httpbin.mesh"
  ],
  "http": [
    {
      "name": "v1-httpbin",
      "match": [
        {
          "headers": {
            "x-intuit-route-name": { "exact": "v1" },
            "x-intuit-source": {
              "exact": "httpbin"
            }
          }
        }
      ],
      "route": [
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 10
        },
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 90
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "all-httpbin",
      "match": [
        {
          "headers": {
            "x-intuit-route-name": { "exact": "all" },
            "x-intuit-source": {
              "exact": "httpbin"
            }
          }
        }
      ],
      "route": [
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 60
        },
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 40
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "v2",
      "match": [{ "headers": { "x-intuit-route-name": { "exact": "v2" } } }],
      "route": [
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 50
        },
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 50
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "v3",
      "match": [{ "headers": { "x-intuit-route-name": { "exact": "v3" } } }],
      "route": [
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 25
        },
        {
          "destination": {
            "host": "e2e-air.httpbin.mesh"
          },
          "weight": 75
        }
      ],
      "timeout": "10s"
    },
    {
      "name": "defaultall-e2e",
      "match": [
        {
          "uri": { "prefix": "/" },
          "authority": { "prefix": "e2e.httpbin.mesh" }
        }
      ],
      "route": [{ "destination": { "host": "e2e.httpbin.mesh" } }],
      "timeout": "10s"
    },
    {
      "name": "defaultall-e2e-air-mesh",
      "match": [
        {
          "uri": { "prefix": "/" },
          "authority": { "prefix": "e2e-air.httpbin.mesh" }
        }
      ],
      "route": [
        { "destination": { "host": "e2e-air.httpbin.mesh" } }
      ],
      "timeout": "10s"
    },
    {
      "name": "defaultall-e2e-air",
      "match": [
        {
          "uri": { "prefix": "/" },
          "authority": { "prefix": "e2e.httpbin.intuit" }
        }
      ],
      "route": [
        { "destination": { "host": "e2e-air.httpbin.mesh" } }
      ],
      "timeout": "10s"
    }
  ]
}
