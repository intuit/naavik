{
  "apiVersion": "admiral.io/v1",
  "kind": "TrafficConfig",
  "metadata": {
    "labels": {
      "asset": "httpbin",
      "env": "e2e"
    },
    "name": "httpbin",
    "namespace": "admiral"
  },
  "spec": {
    "edgeService": {
      "filters": [
        {
          "name": "filter-3387192"
        },
        {
          "name": "filter--671448791"
        }
      ],
      "targetGroups": [
        {
          "name": "Default - Single Swimlane",
          "weights": [
            {
              "name": "Default - All Traffic",
              "weight": 100
            }
          ]
        },
        {
          "name": "DefaultGroup",
          "weights": [
            {
              "name": "Default",
              "weight": 100
            }
          ]
        }
      ],
      "targets": [
        {
          "name": "qal--1040162352",
          "dns": "qal.httpbin.intuit",
          "port": 80,
          "protocol": "http",
          "socketTimeout": 6000
        }
      ],
      "routes": [
        {
          "filterSelector": "filter-3387192",
          "inbound": "/health/full",
          "name": "Health Check",
          "outbound": "/health/full",
          "timeout": 2500,
          "workloadEnvSelectors": ["e2e", "e2e-air"],
          "config": [
            {
              "targetGroupSelector": "Default",
              "targetSelector": "qal--1040162352"
            }
          ]
        },
        {
          "filterSelector": "filter--671448791",
          "inbound": "/*",
          "name": "v1",
          "outbound": "/*",
          "timeout": 2500,
          "workloadEnvSelectors": ["e2e", "e2e-air"],
          "config": [
            {
              "targetGroupSelector": "Default",
              "targetSelector": "qal--1040162352"
            }
          ]
        }
      ]
    },
    "quotaGroup": {
      "totalQuotaGroups": [
        {
          "cpuLimit": 80,
          "description": "Total capacity of service",
          "memoryLimit": 80,
          "name": "Total Throttling Plan",
          "podLevelThreshold": 100,
          "quotas": [
            {
              "algorithm": "scalable",
              "behaviour": "softThrottle",
              "keyType": "any",
              "maxAmount": 100,
              "name": "Total",
              "rule": "/*",
              "timePeriod": "1s"
            }
          ],
          "regionLevelLimit": true,
          "workloadEnvSelectors": ["e2e", "e2e-air"]
        }
      ],
      "appQuotaGroups": [
        {
          "name": "sample app plan-2",
          "description": "Some Description",
          "workloadEnvSelectors": ["e2e-air"],
          "associatedApps": [
            "httpbin",
            "some other app"
          ],
          "quotas": [
            {
              "name": "Everything else rule",
              "rule": "/v1*",
              "maxAmount": 15,
              "timePeriod": "1m",
              "keyType": "appIdOrToken",
              "algorithm": "fast",
              "behaviour": "defaultThrottle",
              "method": ""
            }
          ]
        }
      ]
    },
    "workloadEnvs": ["e2e", "e2e-air"]
  }
}
